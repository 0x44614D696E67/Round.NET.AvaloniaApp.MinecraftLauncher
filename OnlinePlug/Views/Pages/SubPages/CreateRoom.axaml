<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OnlinePlug.Views.Pages.SubPages.CreateRoom">
    <ScrollViewer>
        <StackPanel>
            <StackPanel Margin="10">
                <Label FontSize="20">房间状态：</Label>
                <StackPanel Margin="20,0">
                    <TextBlock TextWrapping="Wrap" Name="RoomStatus">启动中...</TextBlock>
                </StackPanel>
            </StackPanel>
            <StackPanel Margin="10">
                <Label FontSize="20">房间总览：</Label>
                <StackPanel Margin="20,0">
                    <TextBlock TextWrapping="Wrap" Name="RoomNameT">房间名称：</TextBlock>
                    <TextBlock TextWrapping="Wrap" Name="RoomOwner">房间管理：</TextBlock>
                    <TextBlock TextWrapping="Wrap" Name="RoomPort">房间端口：</TextBlock>
                </StackPanel>
            </StackPanel>
        </StackPanel>
    </ScrollViewer>
</UserControl>
