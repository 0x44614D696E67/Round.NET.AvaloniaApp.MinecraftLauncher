<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:FluentAvalonia.UI.Controls;assembly=FluentAvalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Round.NET.AvaloniaApp.MinecraftLauncher.Views.Controls.ContentPanel.ContentPagesPanel">
    <Grid>
        <Label Margin="5,9" FontSize="22" FontWeight="Bold" Content="{Binding Title, RelativeSource={RelativeSource AncestorType=UserControl}}"></Label>
        <controls:NavigationView 
            PaneDisplayMode="Left"
            IsSettingsVisible="False"
            IsPaneOpen="True"
            IsTabStop="False"
            SelectionFollowsFocus="False"
            IsPaneToggleButtonVisible="False"
            HorizontalAlignment="Left" VerticalAlignment="Stretch" Margin="0,51,0,0"
            OpenPaneLength="180"
            Name="View"
            SelectionChanged="NavigationView_OnSelectionChanged">
            <controls:NavigationView.MenuItems>
            </controls:NavigationView.MenuItems>
        </controls:NavigationView>
        <Label Margin="190,50" HorizontalAlignment="Left" VerticalAlignment="Top" FontWeight="Bold" Name="PageTitleLabel" FontSize="18"></Label>
        <controls:Frame Margin="190,80,10,10" Name="MangeFrame">
            
        </controls:Frame>
    </Grid>
</UserControl>
