<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:Round.NET.AvaloniaApp.MinecraftLauncher.Views.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Round.NET.AvaloniaApp.MinecraftLauncher.Views.Pages.Main.Downloads.DownloadAssetsPage">
    <Grid Name="MainGrid">
        <TextBox HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0,10,90,0" Height="32" Name="KeyBox"></TextBox>
        <Button HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,10" Width="80" Height="32" Click="Button_OnClick">搜索</Button>
        <ScrollViewer Margin="0,52,0,0">
            <StackPanel Name="AssetsBox">
                <controls:NullControl HorizontalAlignment="Center" Margin="50"/>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
