<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Round.NET.AvaloniaApp.MinecraftLauncher.Views.Pages.Main.Setting">
    <Grid>
        <Label Margin="7" FontSize="22" FontWeight="Bold">设置</Label>
        <ScrollViewer Margin="10,53,10,10">
            <StackPanel>
                <Expander Header="Java 设置">
                    <StackPanel>
                        <DockPanel>
                            <Label VerticalAlignment="Center">默认 Java：</Label>
                            <ComboBox HorizontalAlignment="Stretch" Name="JavaComboBox" SelectionChanged="JavaComboBox_OnSelectionChanged"></ComboBox>
                        </DockPanel>
                    </StackPanel>
                </Expander>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
