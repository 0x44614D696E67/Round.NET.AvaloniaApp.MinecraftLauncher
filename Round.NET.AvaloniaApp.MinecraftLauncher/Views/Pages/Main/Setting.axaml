<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:Round.NET.AvaloniaApp.MinecraftLauncher.Views.Controls"
             xmlns:controls1="clr-namespace:FluentAvalonia.UI.Controls;assembly=FluentAvalonia"
             xmlns:contentPanel="clr-namespace:Round.NET.AvaloniaApp.MinecraftLauncher.Views.Controls.ContentPanel"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Round.NET.AvaloniaApp.MinecraftLauncher.Views.Pages.Main.Setting">
    <UserControl.Styles>
        <Style Selector="Label">
            <Setter Property="MinWidth" Value="320"></Setter>
        </Style>
    </UserControl.Styles>
    <Grid>
        <contentPanel:ContentPagesPanel Name="ContentPagesPanel" Title="设置" DefaultRoute="GameSetting"/>
    </Grid>
</UserControl>
