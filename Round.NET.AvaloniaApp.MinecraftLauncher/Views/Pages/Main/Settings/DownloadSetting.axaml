<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Round.NET.AvaloniaApp.MinecraftLauncher.Views.Pages.Main.Settings.DownloadSetting">
    <ScrollViewer>
        <StackPanel>
            <StackPanel Margin="0,10,0,0">
                <Label Margin="10,0" FontSize="18" FontWeight="Bold">基础下载设置</Label>

                <DockPanel Margin="20,0,0,0">
                    <Label Name="DownloadThreadsCountLabel" VerticalAlignment="Center">下载线程（256）：</Label>
                    <Slider Name="DownloadThreadsCountSlider" ValueChanged="DownloadThreadsCountSlider_OnValueChanged" Minimum="10" Maximum="512" Value="256"/>
                </DockPanel>
            </StackPanel>
        </StackPanel>
    </ScrollViewer>
</UserControl>
